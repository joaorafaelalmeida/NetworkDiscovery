package PTP;

public enum Flags 
{
	Sync(1), SendTimeSync(2);
	
	int code;
	
	private Flags(int code)
	{
		this.code = code;
	}
	
	public int getCode()
	{
		return code;
	}
	
	public boolean exists(int code)
	{
		for(Flags tmp: this.values())
			if(tmp.getCode() == code)
				return true;
		return false;
	}
}
