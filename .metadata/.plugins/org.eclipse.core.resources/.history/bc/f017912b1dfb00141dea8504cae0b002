package ConClasses;

public class Message 
{
    public static final int DELAY_REQUEST = 1;
    public static final int DELAY_RESPONSE = 2;  
    
    private int msgType;
    private long time;
    private String ip;
    
    public Message (int type, String ip)
    {
        msgType = type;
        this.ip = ip;
    }
    
    public Message (int type, long time)
    {
        msgType = type;
        this.time = time;
    }

    public int getType()
    {
       return msgType;
    }
    
    public long getTime()
    {
       return time;
    }
    
    public String getIp()
    {
    	return ip;
    }

    @Override
    public String toString() 
    {
        return "Message{" + "msgType=" + msgType + ", time=" + time +'}';
    }
}
